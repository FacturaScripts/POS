<div class="modal fade" id="historyModal" tabindex="-1" role="dialog">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">{{ i18n.trans('till-session-operations') }}</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <table class="table search-table table-striped">
                    <thead>
                    <tr>
                        <th scope="col" class="w-25">{{ i18n.trans('code') }}</th>
                        <th scope="col">{{ i18n.trans('customer') }}</th>
                        <th scope="col" class="w-20">{{ i18n.trans('total') }}</th>
                    </tr>
                    </thead>
                    <tbody id="historyResult">

                    </tbody>
                </table>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-dismiss="modal">{{ i18n.trans('close') }}</button>
            </div>
        </div>
    </div>
</div>
